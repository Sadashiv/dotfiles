[user]
        name = Sadashiv H
        email = hsadashiv@zeomega.com
 [apply]
    whitespace = fix
[alias]
    st = status
    ci = commit
    amend = commit --amend
    co = checkout
    br = branch
    lg = log --graph --date=short --date-order
    pur = pull --rebase
    prom = pull --rebase origin master
    pom = push origin master
    what = whatchanged -p
    df = diff --color-words
    di = diff --ignore-space-change
    staged = diff --cached
    unstage = reset HEAD
    credits = shortlog -sn --no-merges
    dw = diff --word
    dc = diff --cahed
    ls = ls-files
     ign = ls-files -o -i --exclude-standard
[color]
    branch = auto
    diff = auto
    interactive = auto
    ui = always
    pager = true
    status = auto
    ui = true
    current = yellow reverse
    local = yellow
    remote = green

[color "status"]
    changed = red
    added = green
    untracked = cyan
[format]
    pretty="%Cgreen%h %Cred%ar %Cblue<%an> %C(yellow)%d %Creset%s %Cred(%ad)"

[core]
        editor = vim
		whitespace=fix,-indent-with-non-tab,trailing-space,cr-at-eol

[diff]
		mnemonicprefix = true
		renames = copies
[merge]
		stat = true
		verbosity = 1
		summary = true
		log = true

[pager]
        status = true
        show-branch = true

[http]
		postBuffer = 52428800

